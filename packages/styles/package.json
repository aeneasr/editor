{
  "name": "ory-editor-styles",
  "version": "0.0.0",
  "main": "lib/",
  "scripts": {
    "build": "postcss -u postcss-import -u postcss-cssnext -c ../../config/postcss/config.js --dir lib/ src/*.css",
    "start": "npm run build -- -w",
    "clean": "rimraf \"lib/*\"",
    "clean:npm": "rimraf \"node_modules/\"",
    "prebuild": "npm run clean"
  },
  "devDependencies": {
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "rimraf": "^2.5.4"
  }
}
