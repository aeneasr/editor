{
  "name": "ory-editor-plugins-html5-video",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "author": "ORY GmbH",
  "license": "GNU Lesser General Public License v3.0",
  "scripts": {
    "generate-d-ts": "tsc -p ./tsconfig.json && rimraf lib/**/*.js && rimraf lib/**/*.js.map",
    "transpile": "babel ./src --out-dir lib --source-maps --copy-files --only **/*.ts,**/*.tsx --extensions .ts,.tsx --ignore **/*.d.ts && rimraf lib/declarations && rimraf lib/tsconfig.json",
    "build": "npm-run-all --parallel build:lib build:css",
    "build:watch": "npm-run-all --parallel build:lib:watch build:css:watch",
    "build:lib": "npm run generate-d-ts && npm run transpile",
    "build:lib:watch": "npm run build:lib -- --watch",
    "build:css": "postcss -c ../../../../postcss.config.js --dir lib/ src/index.css",
    "build:css:watch": "npm run build:css -- -w",
    "docs": "documentation build ./src/**/*.js --format md --github -o ../../../../docs/api/plugins/html5-video.md",
    "prebuild": "npm run clean",
    "clean": "rimraf \"lib/*\"",
    "flow:generate": "flow-typed install --flowVersion=0.79.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "ory-editor-core": "0.0.0",
    "ory-editor-ui": "0.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
