    "lodash.debounce": "^4.0.8",
    "uniforms": "^2.3.0",
    "uniforms-bridge-json-schema": "^2.3.1",
    "uniforms-material": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.3"
  }
