{
  "name": "ory-editor",
  "version": "0.0.0",
  "description": "The next generation platform for creating well-defined, responsive, digital content - as easy as social media.",
  "main": "",
  "scripts": {
    "examples:serve": "cross-env NODE_PATH=. nodemon --config examples/nodemon.json examples/server.js",
    "flow": "flow check --show-all-errors",
    "flow:watch": "npm run watch:js -- -c \"flow check --show-all-errors\"",
    "test": "npm-run-all test:unit test:e2e",
    "test:unit": "cross-env NODE_PATH=. jest",
    "test:e2e": "node bin/nightwatch",
    "test:watch": "npm run test:unit -- --watch",
    "start": "cross-env NODE_PATH=. NODE_ENV=production node examples/server.js",
    "predev": "npm run clean",
    "dev": "npm-run-all --parallel dev:serve build:dev:*",
    "dev:serve": "cross-env NODE_PATH=. nodemon examples/server.js",
    "lint": "eslint \"+(src|test|types)/**/*.js\" --fix",
    "lint:watch": "npm run watch:js -- -c \"npm run lint\"",
    "prebuild": "npm run clean",
    "build": "npm-run-all --parallel build:prod:*",
    "build:dev": "npm-run-all --parallel build:dev:*",
    "build:prod:editor": "cross-env NODE_PATH=. NODE_ENV=production webpack",
    "build:prod:news-article": "cross-env NODE_PATH=. NODE_ENV=production webpack --config ./examples/news-article/webpack.config.js",
    "build:prod:single-page-site": "cross-env NODE_PATH=. NODE_ENV=production webpack --config ./examples/single-page-site/webpack.config.js",
    "build:prod:live-edit": "cross-env NODE_PATH=. NODE_ENV=production webpack --config ./examples/live-edit/webpack.config.js",
    "build:dev:editor": "cross-env NODE_PATH=. webpack --watch",
    "build:dev:news-article": "webpack --watch --config ./examples/news-article/webpack.config.js",
    "build:dev:single-page-site": "webpack --watch --config ./examples/single-page-site/webpack.config.js",
    "build:dev:live-edit": "webpack --watch --config ./examples/live-edit/webpack.config.js",
    "clean": "npm-run-all --parallel clean:*",
    "clean:editor": "rimraf \"public/*\"",
    "clean:news-article": "rimraf \"examples/news-article/public/*\"",
    "clean:single-page-site": "rimraf \"examples/single-page-site/public/*\"",
    "watch:js": "chokidar \"+(examples|src|test|types)/**/*.js\" --initial",
    "docs": "documentation build ./src/editor/**/*.js --format md --github > API.md",
    "docs:watch": "documentation serve ./src/editor/**/*.js --watch",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "v6.2.0",
    "npm": "3.8.9"
  },
  "author": "",
  "license": "Apache 2.0",
  "dependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.16.0",
    "babel-plugin-transform-inline-environment-variables": "6.8.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.17.0",
    "classnames": "2.2.5",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "deep-equal": "1.0.1",
    "device.js": "0.1.2",
    "dnd-core": "2.0.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "flexboxgrid": "6.3.1",
    "json-loader": "0.5.4",
    "katex": "0.6.0",
    "lodash.debounce": "4.0.8",
    "lodash.throttle": "4.1.1",
    "material-ui": "0.16.0",
    "node-uuid": "1.4.7",
    "npm-run-all": "3.1.0",
    "postcss-loader": "1.0.0",
    "ramda": "0.22.1",
    "raven-js": "3.5.1",
    "react": "15.3.2",
    "react-addons-perf": "15.3.2",
    "react-css-modules": "3.7.10",
    "react-dimensions": "1.3.0",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dnd-touch-backend": "0.3.3",
    "react-dom": "15.3.2",
    "react-draggable": "2.2.2",
    "react-dropzone": "3.6.0",
    "react-hotkeys": "0.9.0",
    "react-katex": "^1.0.1",
    "react-native-listener": "1.0.1",
    "react-player": "^0.12.0",
    "react-portal": "2.2.1",
    "react-redux": "4.4.5",
    "react-resizable": "1.4.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.6.0",
    "redux-undo": "1.0.0-beta9",
    "reselect": "2.5.4",
    "rimraf": "2.5.4",
    "selection-position": "1.0.0",
    "semver": "5.3.0",
    "slate": "0.14.15",
    "slate-edit-blockquote": "0.1.2",
    "slate-edit-list": "0.4.2",
    "socket.io": "^1.5.0",
    "style-loader": "0.13.1",
    "superagent": "2.3.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25"
  },
  "devDependencies": {
    "babel-eslint": "7.0.0",
    "chokidar-cli": "1.2.0",
    "deep-equal": "1.0.1",
    "documentation": "4.0.0-beta9",
    "eslint": "3.8.1",
    "eslint-config-ory-am": "3.0.0",
    "eslint-config-ory-am-react": "3.0.0",
    "eslint-plugin-flowtype": "2.20.0",
    "eslint-plugin-react": "5.2.2",
    "flow-bin": "0.33.0",
    "jest": "16.0.1",
    "nightwatch": "0.9.8",
    "nodemon": "1.11.0",
    "react-addons-shallow-compare": "15.3.2",
    "react-test-renderer": "15.3.2",
    "unexpected": "10.18.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    }
  }
}
