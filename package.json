    "react-addons-perf": "^15.2.1",
    "react-css-modules": "3.7.6",
    "react-dimensions": "^1.2.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dnd-touch-backend": "^0.3.0",
    "react-dom": "15.2.0",
    "react-portal": "2.2.1",
    "react-redux": "4.4.5",
