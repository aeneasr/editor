{
  "name": "ory-editor",
  "version": "0.0.0",
  "description": "The next generation platform for creating well-defined, responsive, digital content - as easy as social media.",
  "main": "",
  "scripts": {
    "flow": "flow check",
    "flow:watch": "npm run watch:js -- -c \"flow check\"",
    "test": "npm-run-all test:unit test:e2e",
    "test:unit": "cross-env NODE_PATH=. mocha --require .babel-require --recursive \"+(src|test)/**/*.test.js\"",
    "test:e2e": "node bin/nightwatch",
    "test:watch": "npm run watch:js -- -c \"npm run test:unit -- --reporter min\"",
    "start": "cross-env NODE_PATH=. NODE_ENV=production node src/server",
    "dev": "cross-env NODE_PATH=. nodemon src/server",
    "lint": "eslint \"+(src|test|types)/**/*.js\" --fix",
    "lint:watch": "npm run watch:js -- -c \"npm run lint\"",
    "build": "cross-env NODE_PATH=. NODE_ENV=production webpack",
    "watch:js": "chokidar \"+(src|test)/**/*.js\" --initial",
    "docs": "documentation build ./src/editor/**/*.js --format md --github > API.md",
    "docs:watch": "documentation serve ./src/editor/**/*.js --watch",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "v6.2.0",
    "npm": "3.8.9"
  },
  "author": "",
  "license": "Apache 2.0",
  "dependencies": {
    "autoprefixer": "6.3.7",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.10.2",
    "babel-plugin-transform-inline-environment-variables": "6.8.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.9.0",
    "classnames": "^2.2.5",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "deep-equal": "^1.0.1",
    "device.js": "^0.1.2",
    "dnd-core": "^2.0.2",
    "draft-js": "0.7.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "flexboxgrid": "^6.3.0",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "katex": "^0.6.0",
    "lodash.debounce": "^4.0.6",
    "lodash.throttle": "^4.0.1",
    "material-ui": "0.15.3",
    "node-uuid": "1.4.7",
    "postcss-loader": "0.9.1",
    "ramda": "0.21.0",
