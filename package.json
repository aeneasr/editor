{
  "name": "ory-am-editor",
  "version": "0.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "test": "npm-run-all test:unit test:e2e",
    "test:unit": "cross-env NODE_PATH=. mocha --require .babel-require --recursive \"+(src|test)/**/*.test.js\"",
    "test:e2e": "node bin/nightwatch",
    "test:watch": "npm run watch:js -- -c \"npm run test:unit -- --reporter min\"",
    "start": "cross-env NODE_PATH=. NODE_ENV=production node src/server",
    "dev": "cross-env NODE_PATH=. nodemon src/server",
    "lint": "eslint \"+(src|test)/**/*.js\" --fix",
    "lint:watch": "npm run watch:js -- -c \"npm run lint\"",
    "build": "cross-env NODE_PATH=. NODE_ENV=production webpack",
    "watch:js": "chokidar \"+(src|test)/**/*.js\" --initial",
    "doc": "esdoc -c esdoc.json",
    "doc:watch": "npm run watch:js -- -c \"esdoc -c esdoc.json\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "6.3.6",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-inline-environment-variables": "6.8.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.9.0",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "draft-js": "^0.7.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.21.0",
    "json-loader": "^0.5.4",
    "node-uuid": "^1.4.7",
    "postcss-loader": "0.9.1",
    "ramda": "0.21.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "semver": "^5.2.0",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.9"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.9.0",
    "chokidar-cli": "^1.2.0",
    "deep-equal": "^1.0.1",
    "eslint": "2.13.1",
    "eslint-config-ory-am": "2.0.0",
    "eslint-config-ory-am-react": "2.0.0",
    "eslint-plugin-react": "5.2.2",
    "mocha": "2.5.3",
    "nightwatch": "0.9.4",
    "nodemon": "1.9.2",
    "npm-run-all": "2.2.2",
    "redbox-react": "1.2.9",
    "unexpected": "10.14.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
